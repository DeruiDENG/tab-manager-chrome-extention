{
  "manifest_version": 2,
  "name": "Tab Manager",
  "description": "Find your tab quickly.",
  "version": "0.0.1",
  "offline_enabled": true,
  "permissions": [ "tabs" ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "static/popup.html"
  },
  "background": {
    "scripts": ["static/background.js"]
  }
}
